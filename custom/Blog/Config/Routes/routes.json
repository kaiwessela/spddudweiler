{
	"/": {
		"template": "index",
		"controllers": {
			"ColumnController": {
				"action": "show",
				"identifier": "aktuelles",
				"amount": 5
			},
			"EventController": {
				"action": "list",
				"amount": 10,
				"custom": [
					"future"
				]
			}
		}
	},
	"aktuelles/#?": {
		"template": "posts",
		"controllers": {
			"ColumnController": {
				"action": "show",
				"identifier": "aktuelles",
				"amount": 10,
				"page": "?2",
				"custom": {
					"pagination_structure": "default",
					"pagination_base": "aktuelles"
				}
			}
		}
	},
	"aktuelles/*{9,}": {
		"template": "post",
		"controllers": {
			"PostController": {
				"action": "show",
				"identifier": "?2"
			}
		}
	},
	"a/*{8}": {
		"template": "post",
		"controllers": {
			"PostController": {
				"action": "show",
				"identifier": "?2"
			}
		}
	},
	"bezirksrat/neuigkeiten/#?": {
		"template": "posts",
		"controllers": {
			"ColumnController": {
				"action": "show",
				"identifier": "bezirksrat",
				"amount": 10,
				"page": "?3",
				"custom": {
					"pagination_structure": "default",
					"pagination_base": "bezirksrat/neuigkeiten"
				}
			}
		}
	},
	"ueber-uns": {
		"template": "ueberuns",
		"controllers": {
			"PageController": {
				"action": "show",
				"identifier": "ueber-uns"
			},
			"Vorstand": {
				"alias": "GroupController",
				"action": "show",
				"identifier": "vorstand-ov-dudweiler",
				"amount": 15
			},
			"Jusos": {
				"alias": "GroupController",
				"action": "show",
				"identifier": "jusos-dudweiler"
			}
		}
	},
	"wir-vor-ort": {
		"template": "vorort",
		"controllers": {
			"PageController": {
				"action": "show",
				"identifier": "wir-vor-ort"
			},
			"ColumnController": {
				"action": "show",
				"identifier": "bezirksrat",
				"amount": 3
			},
			"Stadtrat": {
				"alias": "GroupController",
				"action": "show",
				"identifier": "stadtratsmitglieder"
			},
			"Bezirksrat": {
				"alias": "GroupController",
				"action": "show",
				"identifier": "bezirksratsfraktion"
			},
			"ProposalController": {
				"action": "list"
			}
		}
	},
	"*": {
		"template": "page",
		"controllers": {
			"PageController": {
				"action": "show",
				"identifier": "?1"
			}
		}
	}
}
